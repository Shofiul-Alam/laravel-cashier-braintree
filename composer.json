{
    "name": "webnets/laravel-cashier-braintree",
    "description": "Laravel Cashier provides an expressive, fluent interface to Braintree's subscription billing services.",
    "keywords": ["laravel", "braintree", "billing", "cashier"],
    "license": "MIT",
    "authors": [
        {
            "name": "Md Shofiul alam",
            "email": "shofiul@webnets.com.au"
        }
    ],
    "require": {
        "php": "^7.2.0",
        "braintree/braintree_php": "~4.0",
        "dompdf/dompdf": "^0.8.0",
        "illuminate/contracts": "^6.6.2",
        "illuminate/database": "^6.6.2",
        "illuminate/http": "^6.6.2",
        "illuminate/routing": "^6.6.2",
        "illuminate/support": "^6.6.2",
        "illuminate/view": "^6.6.2",
        "nesbot/carbon": "^2.0",
        "symfony/http-kernel": "^4.0|^5.0"
    },
    "require-dev": {
        "ext-json": "*",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "Webnets\\Cashier\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webnets\\Cashier\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webnets\\Cashier\\CashierServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
